pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                echo "Cloning code"
            }
        }

        stage('Build') {
            steps {
                echo "Building project"
            }
        }

        stage('Test') {
            steps {
                echo "Testing application"
            }
        }

        stage('Deploy') {
            steps {
                echo "Deploying app"
            }
        }
    }
}

